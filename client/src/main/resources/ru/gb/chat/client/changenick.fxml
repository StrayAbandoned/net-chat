<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="ru.gb.chat.client.ChangeNickController"
      prefHeight="400.0" prefWidth="600.0">

    <TextField fx:id="newNick" promptText="enter you new nickname" HBox.hgrow="ALWAYS">
        <VBox.margin>
            <Insets top="10.0" left="10.0" right="10.0"/>
        </VBox.margin>
    </TextField>
    <Button VBox.vgrow="ALWAYS" onAction="#changeNick" text="Change nickname">
        <VBox.margin>
            <Insets top="10.0" left="10.0" right="10.0"/>
        </VBox.margin>
    </Button>

</VBox>
